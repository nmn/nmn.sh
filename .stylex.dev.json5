{
  input: ["./source"],
  output: ["./src"],
  cssBundleName: "stylex_bundle.css",
  useCSSLayers: true,
  babelPresets: [
    ["@babel/preset-typescript", { allExtensions: true, isTSX: true }],
  ],
  babelPluginsPre: ["babel-plugin-alias-config"],
  styleXConfig: {
    aliases: {
      "@/*": ["./source/*"],
    },
    dev: true,
    runtimeInjection: false,
  },
}
